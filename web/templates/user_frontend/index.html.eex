<h1>Users</h1>

<p style="line-height: 2.5;">
  <%= for {user, counter} <- Enum.with_index(@users) do %>
    <%= if user.name != "Jane Doe" do %>
      <%= link [fa_icon("user"), " ", user.name],
          to: user_frontend_path(@conn, :show, user),
          class: "btn btn-xs " <> Pan.PodcastFrontendView.btn_cycle(counter) %> &nbsp;
    <% end %>
  <% end %>
</p>