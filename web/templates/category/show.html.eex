<h2>Show category</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @category.title %>
  </li>

  <li>
    <strong>Parent:</strong>
    <%= @category.parent_id %>
  </li>

</ul>

<%= link "Edit", to: category_path(@conn, :edit, @category) %>
<%= link "Back", to: category_path(@conn, :index) %>
